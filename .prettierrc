{
  "tabWidth": 2,
  "printWidth": 80,
  "endOfLine": "auto",
  "arrowParens": "avoid",
  "trailingComma": "none",
  "semi": false,
  "useTabs": false,
  "singleQuote": true,
  "bracketSpacing": true,
  "space-before-function-paren": "error",
  "bracketSameLine": false,
  "plugins": ["prettier-plugin-tailwindcss"]
}
